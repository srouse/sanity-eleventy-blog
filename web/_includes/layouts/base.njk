<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    
    <!-- OSANO GDPR -->
    <script src="https://cmp.osano.com/6oaTaT4PVK0B30Y/2f08b4c3-dae4-47ad-a02e-e2e4fa3e5db1/osano.js"></script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-0LX5ZBPTPZ"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'G-0LX5ZBPTPZ');
    </script>
    <!-- ANALYTICS END -->


    {# <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}"> #}
    {% set css %}
      {% include "style.css" %}
    {% endset %}
    <style>
      {{ css | cssmin | safe }}
    </style>
    <script type="module">
      import {defineComponents} from '/assets/scu-web-components/scw.js';
      // don't technically need it for scw-links...
      /* setTimeout(() => {
        console.log('loaded');
        defineComponents({
          components: [
            'scw-component',
            'scw-link',
            'scw-button'
          ]
        });
      }, 1000 );*/
      defineComponents();
    </script>
    <!--link rel="stylesheet" rel="preload" href="/assets/scu-web-components/scw.css" /-->
    <link rel="stylesheet" rel="preload" href="/assets/scu-web-components/tokens/fonts.css" />
    <link rel="stylesheet" rel="preload" href="/ssr.css" />


  </head>
  <body>
    <scw-flex-v
      scw-gap-2s>
      <scw-link 
        aria-label="Summit Credit Union (text)"
        href="https://www.summitcreditunion.com"
        target="_new">
      </scw-link>
      <scw-link aria-label="Summit Credit Union (text)"
        href="https://www.summitcreditunion.com"
        target="_new">
      </scw-link>
      <scw-element
        scw-text-06
        scw-color-primary
        scw-border-right-checking-06
        scw-padding-4s>
        Hi There scw-text-07
      </scw-element>
      <scw-element
        scw-text-bold-07>
        Hi There scw-text-bold-07
      </scw-element>
      <scw-element
        scw-text-bold-condensed-07>
        Hi There scw-text-bold-condensed-07
      </scw-element>
    </scw-flex-v>
    <header>
      <h1 class="home">
        <a href="{{ '/' | url }}">
          {{ metadata.title }}
        </a>
      </h1>
      <ul class="nav">
        {%- for nav in collections.nav | reverse -%}
          <li 
            class="
              nav-item
              {% if nav.url == page.url %}
                nav-item-active
              {% endif %}">
            <a href="{{ nav.url | url }}">
              {{ nav.data.navtitle }}
            </a>
          </li>
        {%- endfor -%}
      </ul>
    </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>

    <footer>
        <p><em>This is an <a href="https://www.11ty.io/">Eleventy</a> + <a href="https://www.sanity.io">Sanity.io</a> project inspired by the <a href="https://github.com/11ty/eleventy-base-blog"><code>eleventy-base-blog</code> repo</a>.</em></p>
    </footer>

    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>
